	<li class="mb-2">Mandiri berizin dan diawasi oleh Otoritas Jasa Keuangan dan Bank Indonesia.</li>
</ul>
</div>
</div>
</div>
</div>
</div>
</div>
<style type="text/css">.accordion-header {
            cursor: pointer;
        }
        .accordion-icon {
            font-size: 24px;
            color: #6c757d;
            transition: transform 0.3s ease;
        }
        .accordion-header.active .accordion-icon {
            transform: rotate(45deg);
            display: inline-block;
        }
        .accordion-content {
            display: none;
            overflow: hidden;
        }
        .accordion-content.active {
            display: block;
        }
</style>
<script>
        document.addEventListener('DOMContentLoaded', function() {
            // Get all accordion headers
            var accordionHeaders = document.querySelectorAll('.accordion-header');
            
            // Add click event to each header
            accordionHeaders.forEach(function(header) {
                header.addEventListener('click', function() {
                    // Get the target content element (next sibling)
                    var content = this.nextElementSibling;
                    
                    // Check if this header is already active
                    var isActive = this.classList.contains('active');
                    
                    // If active, just close it
                    if (isActive) {
                        this.classList.remove('active');
                        content.classList.remove('active');
                    } else {
                        // Close all other active accordions
                        document.querySelectorAll('.accordion-header.active').forEach(function(activeHeader) {
                            activeHeader.classList.remove('active');
                            activeHeader.nextElementSibling.classList.remove('active');
                        });
                        
                        // Open this accordion
                        this.classList.add('active');
                        content.classList.add('active');
                    }
                });
            });
        });
    </script>
